<hr />
<page-section-head>More From <%= @multicast.collection.title %></page-section-head>
<page-section-content>
<event-image-list>
  <% others_in_collection(@multicast).each do |m| %>
    <%= link_to multicast_path(m.id) do %>
      <event-item class="block-3">
        <%= image_tag m.cover_image %>
      </event-item>
    <% end %>
  <% end %>
</event-image-list>
</page-section-content>