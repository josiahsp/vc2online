<h1>Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Fname</th>
      <th>Lname</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Attendance</th>
      <th>Growgroup</th>
      <th>Growgroupleader</th>
      <th>Dreamteam</th>
      <th>Dreamteamleader</th>
      <th>Details</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.fname %></td>
        <td><%= contact.lname %></td>
        <td><%= contact.email %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.attendance %></td>
        <td><%= contact.growgroup %></td>
        <td><%= contact.growgroupleader %></td>
        <td><%= contact.dreamteam %></td>
        <td><%= contact.dreamteamleader %></td>
        <td><%= contact.details %></td>
        <td><%= link_to 'Show', contact %></td>
        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
        <td><%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact', new_contact_path %>
